<map>
    <entry>
        <string>plugin_status</string>
        <string>development</string>
    </entry>
    <entry>
        <string>plugin_identifier</string>
        <string>org.lockss.plugin.ingenta.hbku.HBKUPlugin</string>
    </entry>
    <entry>
        <string>plugin_version</string>
        <string>1</string>
    </entry>
    <entry>
        <string>plugin_name</string>
        <string>HBKU Plugin</string>
    </entry>
    <entry>
        <string>plugin_feature_version_map</string>
        <map>
            <entry>
                <string>Metadata</string>
                <string>1</string>
            </entry>
            <entry>
                <string>Substance</string>
                <string>1</string>
            </entry>
            <entry>
                <string>Poll</string>
                <string>1</string>
            </entry>
        </map>
    </entry>
    <entry>
        <string>plugin_config_props</string>
        <list>
            <org.lockss.daemon.ConfigParamDescr>
                <key>base_url</key>
                <displayName>Base URL</displayName>
                <description>Usually of the form http://&lt;journal-name&gt;.com/</description>
                <type>3</type>
                <size>40</size>
                <definitional>true</definitional>
                <defaultOnly>false</defaultOnly>
            </org.lockss.daemon.ConfigParamDescr>
            <org.lockss.daemon.ConfigParamDescr>
                <key>volume_name</key>
                <displayName>Volume Name</displayName>
                <type>1</type>
                <size>20</size>
                <definitional>true</definitional>
                <defaultOnly>false</defaultOnly>
            </org.lockss.daemon.ConfigParamDescr>
            <org.lockss.daemon.ConfigParamDescr>
                <key>journal_id</key>
                <displayName>Journal Identifier</displayName>
                <description>Identifier for journal</description>
                <type>1</type>
                <size>40</size>
                <definitional>true</definitional>
                <defaultOnly>false</defaultOnly>
            </org.lockss.daemon.ConfigParamDescr>
        </list>
    </entry>
    <entry>
        <string>au_name</string>
        <string>"HBKU Plugin, Base URL %s, Journal ID %s, Volume %s", base_url, journal_id, volume_name</string>
    </entry>
    <entry>
        <string>au_start_url</string>
        <list>
            <!--HBKU example: https://www.qscience.com/content/journals/ahcsps/lockssissues?volume=2011 -->
            <string>"%scontent/journals/%s/lockssissues?volume=%s", base_url, journal_id, volume_name</string>
        </list>
    </entry>
    <entry>
        <string>au_crawlrules</string>
        <list>
            <!-- HBKU: https://www.qscience.com/content/journals/ahcsps/2011/2 -->
            <string>1, "%scontent/journals/%s/%s/", base_url, journal_id, volume_name</string>
            <!-- https://www.qscience.com/content/journals/10.5339/ahcsps.2011.12 -->
            <string>1, "%scontent/journals/[^/]+/%s\.%s", base_url, journal_id, volume_name</string>
            <!-- https://www.qscience.com/content/journals/10.5339/ahcsps.2011.9?crawler=true&mimetype=application/pdf -->
            <string>1, "%scontent/journals/[^/]+/%s\.%s\.\d+", base_url, journal_id, volume_name</string>
            <string>1, "%s(common|js|css)", base_url</string>
        </list>
    </entry>
    <entry>
        <string>au_substance_url_pattern</string>
        <!-- https://www.qscience.com/content/journals/10.5339/ahcsps.2011.9?crawler=true&mimetype=application/pdf -->
        <!--<string>"%scontent/journals/[^/]+/%s\.%s\.\d+\?crawler=true&amp;mimetype=application/pdf", base_url, journal_id, volume_name</string>-->
        <string>"%scontent/journals/[^/]+/%s\.[0-9]{4}\.\d+", base_url, journal_id</string>
    </entry>
    <entry>
        <string>text/html_link_extractor_factory</string>
        <string>org.lockss.plugin.ingenta.hbku.HBKUHtmlLinkExtractorFactory</string>
    </entry>
    <entry>
        <string>plugin_article_iterator_factory</string>
        <string>org.lockss.plugin.ingenta.hbku.HBKUArticleIteratorFactory</string>
    </entry>
    <entry>
        <string>plugin_article_metadata_extractor_factory</string>
        <string>org.lockss.plugin.ingenta.hbku.HBKUArticleIteratorFactory</string>
    </entry>
    <entry>
        <string>text/html_metadata_extractor_factory_map</string>
        <map>
            <entry>
                <string>*</string>
                <string>org.lockss.plugin.ingenta.hbku.HBKUMetadataExtractorFactory</string>
            </entry>
        </map>
    </entry>
    <entry>
        <string>au_permitted_host_pattern</string>
        <string>"(fonts|ajax)\.googleapis\.com|cdnjs\.cloudflare\.com|fast\.fonts\.net|fonts\.gstatic\.com|cdn\.cookielaw\.org|images\.hindawi\.com|static-01\.hindawi\.com|downloads\.hindawi\.com"</string>
    </entry>
    <entry>
        <string>au_repair_from_peer_if_missing_url_pattern</string>
        <list>
            <string>.+[.](bmp|css|eot|gif|ico|jpe?g|js|otf|png|svg|tif?f|ttf|woff.?)(\?.*)?$</string>
        </list>
    </entry>
    <entry>
        <string>au_url_poll_result_weight</string>
        <list>
            <string>".+[.](bmp|css|eot|gif|ico|jpe?g|js|otf|png|svg|tif?f|ttf|woff.?)(\?.*)?$", 0</string>
        </list>
    </entry>
</map>
