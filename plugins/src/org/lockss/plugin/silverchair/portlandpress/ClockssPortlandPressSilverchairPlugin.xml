    <entry>
        <string>au_crawlrules</string>
        <list>
            <string>1,"^https?://((fonts|ajax)\.(googleapis|gstatic)\.com|cdn\.jsdelivr\.net|cdnjs\.cloudflare\.com|s7\.addthis\.com)/"</string>
            <string>1,"^https?://(%s|[^.]+\.(silverchair-?cdn\.com/|cloudfront\.net/|brightcove\.net/)).*\.(bmp|css|eot|gif|ico|jpe?g|js|otf|png|svg|tif?f|ttf|woff)(\?.*)?$", url_host(base_url)</string>
            <!--
               https://portlandpress.com/bioscirep/article-pdf/38/1/BSR20170862/429223/bsr-2017-0862.pdf
               PDFs are redirected to: https://port.silverchair-cdn.com/port/content_public/journal/bioscirep/38/1/10.1042_bsr20170862/2/bsr-2017-0862.pdf?Expires=1580947780&Signature=I~FNWeNdbhjwOFlE3QF~YnDULgyScF07ypYiJbQLWOqBU2t4dehutEHZf9PMKcgtkgcl2NfekDB2tEGCwdBjSuBps-SVJ45hxvZZgiLMftEprr~Qy8S4b6cOZRLZDUSDxn~FSvug8AzNwCi8Ex-NLO4kxafHgMvI3~W8uB1vpQioF4cnjgt~jl1tySTrUB-JMqMRKAn1uLo60Z-ysykBOhAv-9q4ns8RpE26K~zmbWTvc5715P~cVDi-dYnn23fYR2mwZ7~dd~VjyBm3MmNNpJ49Xi....
           -->
            <string>1,"^https://port\.silverchair-cdn\.com/port/content_public/journal/%s", journal_id</string>
            <!--Reject issues from other year https://portlandpress.com/bioscirep/issue/browse-by-year/1983-->
            <string>2,"%s%s/issue/browse-by-year/\d{4}", base_url, journal_id</string>
            <!--Only accept issues from target year-->
            <string>1,"%s%s/issue/browse-by-year/%d", base_url, journal_id, year</string>
            <!--
                https://portlandpress.com/bioscirep/issue/38/1
                https://portlandpress.com/bioscirep/article-abstract/doi/10.1042/BSR20171148/57181/Thiamine-and-selected-thiamine-antivitamins
                https://portlandpress.com/bioscirep/article/doi/10.1042/BSR20171148/57181/Thiamine-and-selected-thiamine-antivitamins
                https://portlandpress.com/bioscirep/article-abstract/doi/10.1042/BSR20171265/57126/MiR-29a-a-potential-therapeutic-target-and
            -->
            <string>1,"%s%s/(issue|article)/", base_url, journal_id</string>
            <string>1,"%s%s/article-pdf/.*\.pdf$", base_url, journal_id</string>
            <!--
                https://portlandpress.com/Citation/Download?resourceId=57181&resourceType=3&citationFormat=0
                https://portlandpress.com/Citation/Download?resourceId=57181&resourceType=3&citationFormat=1
                https://portlandpress.com/Citation/Download?resourceId=57181&resourceType=3&citationFormat=2
                https://portlandpress.com/Citation/Download?resourceId=57181&resourceType=3&citationFormat=3
            -->
            <string>1,"%sCitation/Download\?resourceId=", base_url</string>
            <!--
                https://portlandpress.com/DownloadFile/DownloadImage.aspx?image=&PPTtype=SlideSet&ar=57181&xsltPath=~/UI/app/XSLT&siteId=1000007
                https://portlandpress.com/DownloadFile/DownloadImage.aspx?image=https://port.silverchair-cdn.com/port/content_public/journal/bioscirep/38/1/10.1042_bsr20171148/2/bsr-2017-1148i001.jpeg?Expires=1580880546&Signature=BIFjn6azjdjetZUOKJLvo1XJ2RCyN9G46td3W1MwZ~UO5pgEEOtQVxtftnfQtyAza0mf0viJ~lvz6P3TYLPx7Bi84zA0otOpc1N0RRxlVYnhDB0gJAe3bYGoM-s6EXmIGDeDorEuIAAq7KaZbnxDvi7yG9MSzzZdhhztoHlG~MDUjx~zKrq97KbmOrk5gOF4McoZJilk9XaK42goi6rwM5vn-VnSqU5KUlmMiUuGA2nZQd-puRuNoBYj-iJcd2ZpMHJvCy9T8YmcQwj263LaDG15eKHYhDlCake9YDLSeNVh5NNOVc8eUafevzfPIbAM8AoNrTkodn3Qql-ckRBe8g__&Key-Pair-Id=APKAIE5G5CRDK6RD3PGA&sec=1503654&ar=57181&xsltPath=~/UI/app/XSLT&imagename=&siteId=1000007
            -->
            <string>1,"%sDownloadFile/DownloadImage.aspx?image=https", base_url</string>
        </list>
    </entry>
