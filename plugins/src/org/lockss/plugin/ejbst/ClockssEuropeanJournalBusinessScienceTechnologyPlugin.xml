<map>
  <entry>
    <string>plugin_status</string>
    <string>development</string>
  </entry>
  <entry>
    <string>plugin_identifier</string>
    <string>org.lockss.plugin.ejbst.ClockssEuropeanJournalBusinessScienceTechnologyPlugin</string>
  </entry>
  <entry>
    <string>plugin_version</string>
    <string>1</string>
  </entry>
  <entry>
    <string>plugin_name</string>
    <string>European Journal of Business Science and Technology Plugin (CLOCKSS)</string>
  </entry>
  <entry>
    <string>plugin_config_props</string>
    <list>
      <org.lockss.daemon.ConfigParamDescr>
        <key>base_url</key>
        <displayName>Base URL</displayName>
        <description>Usually of the form http://&lt;journal-name&gt;.com/</description>
        <type>3</type>
        <size>40</size>
        <definitional>true</definitional>
        <defaultOnly>false</defaultOnly>
      </org.lockss.daemon.ConfigParamDescr>
      <org.lockss.daemon.ConfigParamDescr>
        <key>journal_id</key>
        <displayName>Journal Identifier</displayName>
        <description>Identifier for journal</description>
        <type>1</type>
        <size>40</size>
        <definitional>true</definitional>
        <defaultOnly>false</defaultOnly>
      </org.lockss.daemon.ConfigParamDescr>
      <org.lockss.daemon.ConfigParamDescr>
        <key>year</key>
        <displayName>Year</displayName>
        <description></description>
        <type>4</type>
        <size>4</size>
        <definitional>true</definitional>
        <defaultOnly>false</defaultOnly>
      </org.lockss.daemon.ConfigParamDescr>
    </list>
  </entry>
  <entry>
    <string>plugin_feature_version_map</string>
    <map>
      <entry>
        <string>Metadata</string>
        <string>1</string>
      </entry>
      <entry>
        <string>Poll</string>
        <string>1</string>
      </entry>
      <entry>
        <string>Substance</string>
        <string>1</string>
      </entry>
    </map>
  </entry>
  <entry>
    <string>au_name</string>
    <string>"European Journal of Business Science and Technology Plugin (CLOCKSS), Base URL %s, Journal_ID %s, Year %d", base_url, journal_id, year</string>
  </entry>
  <entry>
    <string>au_start_url</string>
    <!--https://www.ejobsat.cz/magno/ejo/2018/mn1.php-->
    <string>"%smagno/%s/%d/mn1.php", base_url, journal_id, year</string>
  </entry>
  <entry>
    <string>au_crawlrules</string>
    <list>
      <string>1,"^https://pro.fontawesome.com/.*\.(bmp|css|eot|gif|ico|jpe?g|js|png|svg|tif?f|ttc|ttf|woff.?|dfont|otf)"</string>
      <string>1,"^https://code.jquery.com/.*\.js"</string>
      <string>1,"https://s7.addthis.com/js/.*\.js"</string>
      <string>1,"^https://fonts.googleapis.com/css"</string>
      <string>1,"^https://fonts.gstatic.com/.*\.css"</string>
      <string>1,"^%sartkey/ejo-%d", base_url, year</string>
      <string>1,"^%s(pdfs|img|etc)/ejo", base_url</string>
      <!--https://www.ejobsat.cz/clanek.php?action=refdn&rfmt=ris&artkey=ejo-201801-0003-->
      <string>1,"^%sclanek.php\?action=refdn&amp;rfmt=ris&amp;artkey=%s-%d", base_url, journal_id, year</string>
    </list>
  </entry>
  <entry>
    <string>au_substance_url_pattern</string>
    <list>
      <!--https://www.ejobsat.cz/pdfs/ejo/2020/01/05.pdf-->
      <string>"^%spdfs/%s/%d/.*\.pdf$", base_url, journal_id, year</string>
    </list>
  </entry>
  <entry>
    <string>text/html_filter_factory</string>
    <string>org.lockss.plugin.ejbst.EuropeanJournalBusinessScienceTechnologyHtmlFilterFactory</string>
  </entry>
  <entry>
    <string>plugin_article_iterator_factory</string>
    <string>org.lockss.plugin.ejbst.EuropeanJournalBusinessScienceTechnologyArticleIteratorFactory</string>
  </entry>  
  <entry>
    <string>plugin_article_metadata_extractor_factory</string>
    <string>org.lockss.plugin.ejbst.EuropeanJournalBusinessScienceTechnologyArticleIteratorFactory</string>
  </entry>
  <entry>
    <string>text/html_metadata_extractor_factory_map</string>
    <map>
      <entry>
      <string>*</string>
        <string>org.lockss.plugin.ejbst.EuropeanJournalBusinessScienceTechnologyMetadataExtractorFactory</string>
      </entry>
    </map>
  </entry>
  <entry>
    <string>au_repair_from_peer_if_missing_url_pattern</string>
    <list>
      <string>.*\.(bmp|css|eot|gif|ico|jpe?g|js|png|svg|tif?f|ttc|ttf|woff.?|dfont|otf)(_v[0-9]+|\?v=[0-9.]+|\?[a-z0-9]+)</string>
    </list>
  </entry>
  <entry>
    <string>au_url_poll_result_weight</string>
    <list>
      <string>.*\.(bmp|css|eot|gif|ico|jpe?g|js|png|svg|tif?f|ttc|ttf|woff.?|dfont|otf)(_v[0-9]+|\?v=[0-9.]+|\?[a-z0-9]+), 0</string>
    </list>
  </entry>
  <entry>
    <string>au_permitted_host_pattern</string>
    <list>
      <string>pro\.fontawesome\.com</string>
      <string>s7\.addthis\.com</string>
      <string>fonts\.googleapis\.com</string>
      <string>fonts\.gstatic\.com</string>
      <string>code\.jquery\.com</string>
    </list>
  </entry>
</map>    
