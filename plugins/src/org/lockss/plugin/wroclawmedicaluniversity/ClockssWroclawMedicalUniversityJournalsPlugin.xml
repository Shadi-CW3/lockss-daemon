        <string>1</string>
    </entry>
    <entry>
        <string>plugin_name</string>
        <string>Wroclaw Medical University Journals Plugin (CLOCKSS)</string>
    </entry>
    <entry>
        <string>plugin_feature_version_map</string>
        <map>
            <entry>
                <string>Substance</string>
