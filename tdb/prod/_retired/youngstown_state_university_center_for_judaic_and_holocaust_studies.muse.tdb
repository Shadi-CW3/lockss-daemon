{
  publisher <
    name = Youngstown State University Center for Judaic and Holocaust Studies
  >

  plugin = org.lockss.plugin.projmuse.ProjectMusePlugin
  implicit < status ; year ; name ; param[volume] >

  {
    title <
      name = Journal of Jewish Identities ;
      issn = 1946-2522 ;
      eissn = 1939-7941 ;
      issnl = 1939-7941
    >

    param[journal_dir] = journal_of_jewish_identities
    attr[journal_id] = jji

    {

      param[base_url] = http://muse.jhu.edu/

      au < down ; 2008 ; Journal of Jewish Identities Issue 1 ; 1 >
      au < down ; 2009 ; Journal of Jewish Identities Issue 2 ; 2 >
      au < down ; 2010 ; Journal of Jewish Identities Issue 3 ; 3 >
      au < down ; 2011 ; Journal of Jewish Identities Issue 4 ; 4 >
      au < down ; 2012 ; Journal of Jewish Identities Issue 5 ; 5 >
      au < down ; 2013 ; Journal of Jewish Identities Issue 6 ; 6 >

    }

    {

      param[base_url] = https://muse.jhu.edu/

      au < down ; 2014 ; Journal of Jewish Identities Issue 7 ; 7 >
      au < down ; 2015 ; Journal of Jewish Identities Issue 8 ; 8 >
      au < doNotProcess ; 2016 ; Journal of Jewish Identities Issue 9 ; 9 >

    }

  }

}
