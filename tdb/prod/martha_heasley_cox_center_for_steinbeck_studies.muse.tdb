{
  publisher <
    name = Martha Heasley Cox Center for Steinbeck Studies
  >

  implicit < status ; year ; name ; param[volume] >

  plugin = org.lockss.plugin.projmuse.ProjectMusePlugin

  {
    title <
      name = Steinbeck Studies ;
      issn = 1551-6903 ;
      eissn = 0898-7734 ;
      issnl = 1551-6903
    >

    param[journal_dir] = steinbeck_studies
    attr[journal_id] = stn

    {

      param[base_url] = http://muse.jhu.edu/

      au < down ; 2004 ; Steinbeck Studies Volume 15 ; 15 >
      au < down ; 2005 ; Steinbeck Studies Volume 16 ; 16 >

    }

 
  }

  {
    title <
      name = Steinbeck Review ;
      issn = 1546-007X ;
      eissn = 1754-6087 ;
      issnl = 1546-007X
    >

    param[journal_dir] = steinbeck_review
    attr[journal_id] = str
    # no issue from volume 5 to 9

    {

      param[base_url] = http://muse.jhu.edu/

      au < superseded ; 2006 ; Steinbeck Review Volume 3 [superseded] ; 3 >
      au < superseded ; 2007 ; Steinbeck Review Volume 4 [superseded] ; 4 >

    }

    {

      param[base_url] = https://muse.jhu.edu/

      au < superseded ; 2013 ; Steinbeck Review Volume 10 [superseded] ; 10 >
      au < superseded ; 2014 ; Steinbeck Review Volume 11 [superseded] ; 11 >
      au < superseded ; 2015 ; Steinbeck Review Volume 12 [superseded] ; 12 >
      au < doNotProcess ; 2016 ; Steinbeck Review Volume 13 ; 13 >

    }

  }

}
