{
  publisher <
    name = Advertising Educational Foundation
  >

  implicit < status ; year ; name ; param[volume] >

  plugin = org.lockss.plugin.projmuse.ProjectMusePlugin

  {
    title <
      name = Advertising & Society Quarterly ;
      eissn = 1534-7311 ;
      issnl = 1534-7311
    >
    #title changed to 'Quarterly' at Volume 18.
    param[journal_dir] = advertising_and_society_review
    attr[journal_id] = asr

    {

      param[base_url] = http://muse.jhu.edu/

      au < down ; 2000 ; Advertising & Society Review Volume 1 ; 1 >
      au < down ; 2001 ; Advertising & Society Review Volume 2 ; 2 >
      au < down ; 2002 ; Advertising & Society Review Volume 3 ; 3 >
      au < down ; 2003 ; Advertising & Society Review Volume 4 ; 4 >
      au < down ; 2004 ; Advertising & Society Review Volume 5 ; 5 >
      au < down ; 2005 ; Advertising & Society Review Volume 6 ; 6 >
      au < down ; 2006 ; Advertising & Society Review Volume 7 ; 7 >
      au < down ; 2007 ; Advertising & Society Review Volume 8 ; 8 >
      au < down ; 2008 ; Advertising & Society Review Volume 9 ; 9 >
      au < down ; 2009 ; Advertising & Society Review Volume 10 ; 10 >
      au < down ; 2010-2011 ; Advertising & Society Review Volume 11 ; 11 >
      au < down ; 2011-2012 ; Advertising & Society Review Volume 12 ; 12 >
      au < down ; 2012-2013 ; Advertising & Society Review Volume 13 ; 13 >

    }

    {

      param[base_url] = https://muse.jhu.edu/

      au < down ; 2013-2014 ; Advertising & Society Review Volume 14 ; 14 >
      au < down ; 2014-2015 ; Advertising & Society Review Volume 15 ; 15 >
      au < doNotProcess ; 2015-2016 ; Advertising & Society Review Volume 16 ; 16 >
      au < doNotProcess ; 2016-2017 ; Advertising & Society Review Volume 17 ; 17 >

    }

  }

}
