{
  publisher <
    name = Ecole des Hautes Etudes en Sciences Sociales
  >

  implicit < status ; year ; name ; param[volume] >

  plugin = org.lockss.plugin.projmuse.ProjectMusePlugin

  {
    title <
      name = Annales. Histoire, Sciences sociales (French Edition) ;
      issn = 0395-2649 ;
      eissn = 1953-8146
    >

    param[journal_dir] = annales_histoire_sciences_sociales
    attr[journal_id] = ahs
    
    {

      param[base_url] = https://muse.jhu.edu/

      au < down ; 2014 ; Annales Volume 69 ; 69 >
      au < down ; 2015 ; Annales Volume 70 ; 70 >

      au < doNotProcess ; 2016 ; Annales Volume 71 ; 71 >

    }

  }

}
