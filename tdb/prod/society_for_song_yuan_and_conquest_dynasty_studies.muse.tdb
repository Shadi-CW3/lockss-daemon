{
  publisher <
    name = Society for Song, Yuan, and Conquest Dynasty Studies
  >
  plugin = org.lockss.plugin.projmuse.ProjectMusePlugin
  implicit < status ; year ; name ; param[volume] >

  {
    title <
      name = Journal of Song-Yuan Studies ;
      issn = 1059-3152 ;
      eissn = 2154-6665 ;
      issnl = 1059-3152
    >

    param[journal_dir] = journal_of_song_yuan_studies
