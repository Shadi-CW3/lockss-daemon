{

  publisher <
    name = John Wiley and Sons ;
    info[contract] = 2008 ;
    info[tester] = S
  >
  
  provider = Wiley-Blackwell
  
  {
  
    title <
      name = John Wiley and Sons Source Content
    >
    
    {
      
      plugin = org.lockss.plugin.wiley.ClockssWileySourcePlugin
      param[base_url] = http://clockss-ingest.lockss.org/sourcefiles/wiley-released/
      hidden[proxy] = DIRECT
      implicit < status ; status2 ; year ; name ; param[year] >
      
      au < readySource ; finished ; 2011 ; Wiley Source Content 2011 ; 2011 >
      au < readySource ; finished ; 2012 ; Wiley Source Content 2012 ; 2012 >
      au < readySource ; frozen ; 2013 ; Wiley Source Content 2013 ; 2013 >
      au < readySource ; finished ; 2014 ; Wiley Source Content 2014 ; 2014 >
      au < readySource ; finished ; 2015 ; Wiley Source Content 2015 ; 2015 >
      au < readySource ; finished ; 2016 ; Wiley Source Content 2016 ; 2016 >
      au < readySource ; frozen ; 2017 ; Wiley Source Content 2017 ; 2017 >
      #no more AUs using this plugin
      
    }
  
    {
      
      plugin = org.lockss.plugin.wiley.ClockssWileyDirSourcePlugin
      param[base_url] = http://clockss-ingest.lockss.org/sourcefiles/wiley-released/
      hidden[proxy] = DIRECT
      implicit < status ; status2 ; year ; name ; param[directory] >
      
    
      au < readySource ; crawling ; 2018 ; Wiley Source Content 2018_1 ; 2018_1 >
      au < readySource ; crawling ; 2018 ; Wiley Source Content 2018_2 ; 2018_2 >
      au < readySource ; crawling ; 2018 ; Wiley Source Content 2018_3 ; 2018_3 >
      
    }
  }

}