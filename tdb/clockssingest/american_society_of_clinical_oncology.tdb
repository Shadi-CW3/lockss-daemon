{
  publisher <
    name = American Society of Clinical Oncology ;
    info[contract] = 2017 ;
    info[back] = 1980 ;
    info[tester] = 5 ;
    info[poller] = 3

  >

    plugin = org.lockss.plugin.atypon.asco.ClockssAscoJournalsPlugin
    implicit < status ; status2 ; year ; name ; param[volume_name] ; hidden[proxy] >
    # waiting for this journal to move to clockss.
    
    #moving to Atypon October 2016
    param[base_url] = http://ascopubs.org/

  {
    title <
      name = Journal of Clinical Oncology ;
      issn = 0732-183X ;
      issnl = 0732-183X
    >

      #param[base_url] = http://jco.ascopubs.org/
      param[journal_id] = jco
      # NOT only abstract_only but supplemental issues can be  abstract_only (V34)
      # and iterator only counts these articles if coverage depth is set
      # this will work with both but we should consider "abstract_partial"
      attr[au_coverage_depth] = abstracts
      	      

